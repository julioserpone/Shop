{
    "license": "MIT",
    "name": "antvel/shop",
    "homepage": "http://antvel.com",
    "type": "laravel-bundle",
    "description": "The Antvel e-commerce Framework.",
    "keywords": ["antvel", "laravel", "e-commerce", "ecommerce", "shop", "portal", "market", "store"],
    "minimum-stability": "dev",

    "authors": [
        {
            "name": "Gustavo Ocanto",
            "email": "gustavoocanto@gmail.com"
        }
    ],

    "support": {
        "source": "https://github.com/ant-vel/Shop",
        "issues": "https://github.com/ant-vel/Shop/issues"
    },

    "require": {
        "php": "~7.0|~7.1",
        "nesbot/carbon": "^1.21",
        "illuminate/support": "5.5.0",
        "illuminate/http": "5.5.0",
        "illuminate/database": "5.5.0",
        "illuminate/validation": "5.5.0",
        "illuminate/contracts": "5.5.0",
        "illuminate/queue": "5.5.0",
        "illuminate/routing": "5.5.0",
        "illuminate/filesystem": "5.5.0",
        "illuminate/pagination": "5.5.0",
        "illuminate/container": "5.5.0",
        "illuminate/session": "5.5.0",
        "illuminate/bus": "5.5.0",
        "illuminate/mail": "5.5.0",
        "illuminate/notifications": "5.5.0",
        "illuminate/hashing": "5.5.0",
        "illuminate/auth": "5.5.0",
        "intervention/image": "^2.4",
        "ramsey/uuid": "^3.7"
    },

    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "3.5.0",
        "mockery/mockery": "0.9.*",
        "sebastian/diff": "^1.4"
    },

    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Antvel\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Antvel\\Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },

    "scripts": {
        "test": "./vendor/bin/phpunit"
    },

    "extra": {
        "laravel": {
            "providers": [
                "Antvel\\AntvelServiceProvider"
            ]
        }
    }
}
