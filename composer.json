{
    "license": "MIT",
    "name": "julioserpone/epikfy-framework",
    "homepage": "http://www.epikfy.com",
    "type": "laravel-bundle",
    "description": "The Epikfy e-commerce Framework",
    "keywords": ["epikfy", "laravel", "e-commerce", "ecommerce", "shop", "portal", "market", "store"],
    "minimum-stability": "dev",

    "authors": [
        {
            "name": "Julio Hernandez",
            "email": "juliohernandezs@gmail.com"
        }
    ],

    "support": {
        "source": "https://github.com/julioserpone/epikfy-shop",
        "issues": "https://github.com/julioserpone/epikfy-shop/issues"
    },

    "require": {
        "php": "~7.0|~7.1",
        "nesbot/carbon": "^1.21",
        "illuminate/support": "5.5.*",
        "illuminate/http": "5.5.*",
        "illuminate/database": "5.5.*",
        "illuminate/validation": "5.5.*",
        "illuminate/contracts": "5.5.*",
        "illuminate/queue": "5.5.*",
        "illuminate/routing": "5.5.*",
        "illuminate/filesystem": "5.5.*",
        "illuminate/pagination": "5.5.*",
        "illuminate/container": "5.5.*",
        "illuminate/session": "5.5.*",
        "illuminate/bus": "5.5.*",
        "illuminate/mail": "5.5.*",
        "illuminate/notifications": "5.5.*",
        "illuminate/hashing": "5.5.*",
        "illuminate/auth": "5.5.*",
        "intervention/image": "^2.4",
        "ramsey/uuid": "^3.7"
    },

    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "3.5.0",
        "mockery/mockery": "0.9.*",
        "sebastian/diff": "^1.4"
    },

    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Epikfy\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Epikfy\\Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },

    "scripts": {
        "test": "./vendor/bin/phpunit"
    },

    "extra": {
        "laravel": {
            "providers": [
                "Epikfy\\EpikfyServiceProvider"
            ]
        }
    }
}
